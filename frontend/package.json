{
  "name": "zamato-microfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:user\" \"npm run dev:restaurant\"",
    "dev:user": "cd apps/user-ui && npm run dev",
    "dev:restaurant": "cd apps/restaurant-ui && npm run dev",
    "build": "concurrently \"npm run build:user\" \"npm run build:restaurant\"",
    "build:user": "cd apps/user-ui && npm run build",
    "build:restaurant": "cd apps/restaurant-ui && npm run build",
    "preview": "concurrently \"npm run preview:user\" \"npm run preview:restaurant\"",
    "preview:user": "cd apps/user-ui && npm run preview",
    "preview:restaurant": "cd apps/restaurant-ui && npm run preview",
    "setup": "npm install && npm run setup:user && npm run setup:restaurant",
    "setup:user": "cd apps/user-ui && npm install",
    "setup:restaurant": "cd apps/restaurant-ui && npm install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}